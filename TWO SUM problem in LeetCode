class Solution
{
    public int[] twoSum(int[] nums, int target) 
    {
        int[] arr=new int[2]; 
        int j,k,n;
		for(j=0;j<nums.length;j++)
		{
		    for(k=j+1;k<nums.length;k++)
		    {
		        if(nums[j]+nums[k]==target)
		        {
		           arr[0]=j;
                   arr[1]=k;
                            
		        }
		    }
        }
        return arr;
    } 
}
